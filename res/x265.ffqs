vbv-maxrate
text=Maximum local bitrate (kbits/sec)
type=integer
range=0 max
default=0

vbv-init
text=Initial buffer occupancy
type=float
range=0 1
default=0.9

vbv-end
text=Final buffer fullness
type=float
range=0 1
default=0

#vbv-end-fr-adj
