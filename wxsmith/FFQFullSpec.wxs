<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="FFQFullSpec">
		<centered>1</centered>
		<object class="wxFlexGridSizer" variable="MainSizer" member="yes">
			<cols>1</cols>
			<rows>2</rows>
			<object class="sizeritem">
				<object class="wxScrolledWindow" name="ID_SCROLLWIN" variable="ScrollWin" member="yes">
					<style>wxVSCROLL</style>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BtnSizer" member="yes">
					<object class="sizeritem">
						<object class="wxButton" name="ID_TESTBTN" variable="TestBtn" member="yes">
							<label>T</label>
							<extra_code>TestBtn-&gt;SetLabel(FFQS(SID__FULLSPEC__TEST));</extra_code>
							<handler function="ActionClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="spacer">
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="spacer">
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_OKBTN" variable="OkBtn" member="yes">
							<label>O</label>
							<default>1</default>
							<extra_code>OkBtn-&gt;SetLabel(FFQS(SID__COMMON__OK));</extra_code>
							<handler function="ActionClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_CANCELBTN" variable="CancelBtn" member="yes">
							<label>C</label>
							<extra_code>CancelBtn-&gt;SetLabel(FFQS(SID__COMMON__CANCEL));</extra_code>
							<handler function="ActionClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<option>1</option>
			</object>
		</object>
		<object class="wxFileDialog" variable="FileDlg" member="yes">
			<style>wxFD_DEFAULT_STYLE|wxFD_OPEN|wxFD_FILE_MUST_EXIST</style>
		</object>
	</object>
</wxsmith>
