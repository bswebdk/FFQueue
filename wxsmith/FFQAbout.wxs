<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="FFQAbout">
		<title>About FFQueue</title>
		<centered>1</centered>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>1</cols>
			<rows>3</rows>
			<growablecols>0</growablecols>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>2</cols>
					<rows>1</rows>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxStaticBitmap" name="ID_STATICBITMAP" variable="StaticBitmap" member="yes">
							<size>100,100</size>
							<style>wxNO_BORDER</style>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
							<cols>1</cols>
							<rows>3</rows>
							<growablecols>0</growablecols>
							<object class="sizeritem">
								<object class="wxStaticText" name="wxID_ANY" variable="Hdr" member="yes">
									<label>ffq</label>
									<font>
										<size>26</size>
										<style>normal</style>
										<weight>bold</weight>
										<underlined>0</underlined>
										<family>swiss</family>
										<face>Arial</face>
									</font>
									<extra_code>Hdr-&gt;SetLabel(GUI__1);</extra_code>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>5</border>
							</object>
							<object class="sizeritem">
								<object class="wxFlexGridSizer" variable="FlexGridSizer5" member="no">
									<cols>2</cols>
									<rows>1</rows>
									<growablecols>0</growablecols>
									<growablerows>0</growablerows>
									<object class="sizeritem">
										<object class="wxStaticText" name="wxID_ANY" variable="Info" member="yes">
											<label>gui</label>
											<extra_code>Info-&gt;SetLabel(GUI__2);</extra_code>
										</object>
										<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxHyperlinkCtrl" name="wxID_ANY" variable="Link" member="no">
											<label>www</label>
											<extra_code>Link-&gt;SetLabel(GUI__3);&#x0A;Link-&gt;SetURL(GUI__4);</extra_code>
										</object>
										<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxFlexGridSizer" variable="FlexGridSizer4" member="no">
									<cols>2</cols>
									<rows>1</rows>
									<growablecols>0</growablecols>
									<object class="sizeritem">
										<object class="wxStaticText" name="wxID_ANY" variable="Bacon" member="yes">
											<label>100%</label>
											<extra_code>Bacon-&gt;SetLabel(GUI__5);</extra_code>
										</object>
										<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxPanel" name="ID_ANIMPANEL" variable="AnimPanel" member="yes">
											<style>wxNO_BORDER|wxTAB_TRAVERSAL</style>
										</object>
										<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxTextCtrl" name="ID_TEXTCTRL" variable="TextCtrl" member="yes">
					<size>500,300</size>
					<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_RICH|wxSTATIC_BORDER</style>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
